        JButton listAllSpeciesButton = new JButton("Lister les esp\u00E8ces");
        listAllSpeciesButton.setRequestFocusEnabled(false);
        listAllSpeciesButton.addMouseListener(new MouseAdapter() {

            @Override
            public void mouseEntered(MouseEvent e) {
                listAllSpeciesButton.setBackground(new Color(49, 49, 49));
            }
            @Override
            public void mouseExited(MouseEvent e) {
                listAllSpeciesButton.setBackground(new Color(29, 29, 29));
            }
        });
        listAllSpeciesButton.setMargin(new Insets(2, 100, 2, 14));
