	public static void loadIntoTable(List<Object> insertedInto, Object  arrayToInsert ) {
		insertedInto.add(arrayToInsert);
	}

	public static void refreshTable(JTable table, DefaultTableModel model, List<Object> tableData) {
		for (int i = 0; i < tableData.size(); i++) {
			model.addRow((Object[]) tableData.get(i));
		}
		table.setModel(model);
	}